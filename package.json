{
	"name": "obsidian-tag-renamer",
	"version": "1.0.0",
	"description": "Rename tags across your entire vault with ease",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"test": "npm run build && node -e \"require('./main.js'); require('./src/tests/TestRunner.js').testRunner.runAllTests()\"",
		"test:watch": "npm run build && npm run test",
		"test:tagprocessor": "npm run build && node -e \"require('./main.js'); require('./src/tests/TestRunner.js').testRunner.runTagProcessorTests()\"",
		"test:fileservice": "npm run build && node -e \"require('./main.js'); require('./src/tests/TestRunner.js').testRunner.runFileServiceTests()\"",
		"test:integration": "npm run build && node -e \"require('./main.js'); require('./src/tests/TestRunner.js').testRunner.runIntegrationTests()\"",
		"test:report": "npm run build && node -e \"console.log(require('./src/tests/TestRunner.js').testRunner.generateTestReport())\"",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "0BSD",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
